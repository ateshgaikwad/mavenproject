pipeline{
    agent any
    stages{
        stage('scm checkout'){
            steps{git branch: 'master', url: 'https://github.com/ateshgaikwad/mavenproject.git'}
        }

        stage(compile){
            steps{withMaven(globalMavenSettingsConfig: '', jdk: 'JDK', maven: 'Maven', mavenSettingsConfig: '', traceability: true) 
            {
             sh 'mvn compile'
            }
            }
        }
        stage(test){
            steps{withMaven(globalMavenSettingsConfig: '', jdk: 'JDK', maven: 'Maven', mavenSettingsConfig: '', traceability: true) 
            {
             sh 'mvn test'
            }
            }
        }
        

    }
}